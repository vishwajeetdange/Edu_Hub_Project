@model IEnumerable<MVC_EduHub_Project.Models.GetCourseIdCourseName>

@{
    ViewData["Title"] = "List Of Enquiry For Courses";
    Layout = "~/Views/Shared/_LayoutEducator.cshtml";
}

<h2 class="text-center">Enquiry For My Courses</h2>

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>
                CourseName
                @* @Html.DisplayNameFor(model => model.Title) *@
                </th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem
                             => item.Title)</td>
                <td>
                    <button class="btn btn-warning btn-sm">
                        <a class="text-white" asp-controller="Enquiry" asp-action="GetEnquiryByCourseId" asp-route-id="@item.CourseId">Check
                            Enquiry</a>
                    </button>
                </td>
            </tr>
        }
    </tbody>
</table>

<div class="text-center">
    <a asp-controller="User" asp-action="EducatorIndex" class="btn btn-secondary">Back to DashBord</a>
</div>